<h1>Listing stylesheets</h1>

<table>
  <tr>
    <th>Created of</th>
    <th>Name</th>
    <th>Owner</th>
    <th>Created on</th>
    <th>Updated on</th>
  </tr>

<% for stylesheet in @stylesheets %>
  <tr>
    <td><%=h stylesheet.created_of %></td>
    <td><%=h stylesheet.name %></td>
    <td><%=h stylesheet.owner %></td>
    <td><%=h stylesheet.created_on %></td>
    <td><%=h stylesheet.updated_on %></td>
    <td><%= link_to 'Show', stylesheet %></td>
    <td><%= link_to 'Edit', edit_stylesheet_path(stylesheet) %></td>
    <td><%= link_to 'Destroy', stylesheet, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New stylesheet', new_stylesheet_path %>
