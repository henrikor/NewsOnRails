<h1>Listing group_members</h1>

<table>
  <tr>
  <% for column in GroupMember.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for group_member in @group_members %>
  <tr>
  <% for column in GroupMember.content_columns %>
    <td><%=h group_member.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => group_member %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => group_member %></td>
    <td><%= link_to 'Destroy', {:action => 'destroy', :id => group_member}, :confirm => 'Are you sure?' %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @group_member_pages.current.previous } if @group_member_pages.current.previous %>
<%= link_to 'Next page', { :page => @group_member_pages.current.next } if @group_member_pages.current.next %> 

<br />

<%= link_to 'New group_member', :action => 'new' %>
